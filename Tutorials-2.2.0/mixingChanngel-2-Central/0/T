FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           uniform 300;
    }

    inlet2
    {
        type            fixedValue;
        value           uniform 300;
    }

    outlet
    {
	type		fixedGradient;
	gradient	uniform 0;
    }

    walls
    {
        type            slip;
    }


    frontAndBack
    {
        type            empty;
    }
}

